{
  "name": "redberry-bootcamp-project",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "commit": "npx git-cz",
    "dev": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "prepare": "husky",
    "pretty": "npx prettier . --write",
    "preview": "vite preview",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --config .eslintrc.cjs --fix --report-unused-disable-directives --max-warnings 0"
    ],
    "*.{js,jsx,ts,tsx,css,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
